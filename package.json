{
  "name": "resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "browser-sync start --server --files '**/*'",
    "start": "yarn build",
    "watch": "yarn serve & yarn w-css",
    "serve": "browser-sync start --server --serveStatic dist --files 'dist/*.css, index.html'",
    "css": "postcss -c ./.postcssrc.json",
    "w-css": "postcss -c ./.postcssrc.json -w --map",
    "lint-css": "stylelint ./src/**/*.css",
    "build": "postcss -c ./.postcssrc.json && cpx ./index.html build/ && cpx ./dist/**.css build/dist/ && cpx ./static/**.png build/static/",
    "deploy": "yarn build && gh-pages -d build",
    "heroku-prebuild": "yarn"
  },
  "author": "hrfmmymt",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "browser-sync": "^2.18.8",
    "cpx": "^1.5.0",
    "gh-pages": "^0.12.0",
    "postcss": "^5.2.6",
    "postcss-cli": "^2.6.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^9.1.0",
    "postcss-nested": "^1.0.0",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.9.0"
  },
  "dependencies": {
    "express": "3.3.4"
  }
}
